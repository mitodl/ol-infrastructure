{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'local>mitodl/.github:renovate-config',
  ],
  packageRules: [
    {
      matchManagers: [
        'custom.regex',
      ],
      matchFileNames: [
        'dockerfiles/ocw/node-hugo/Dockerfile',
      ],
      automerge: false,
    },
    {
      groupName: 'pulumi-plugins',
      matchPackageNames: [
        'pulumi{/,}**',
      ],
    },
  ],
  pip_requirements: {
    managerFilePatterns: [
      '/dockerfiles/openedx-edxapp/pip_package_lists/.*?/.*.txt/',
    ],
  },
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/^src/bridge/lib/versions.py$/',
        '/^dockerfiles/ocw/node-hugo/Dockerfile$/',
      ],
      matchStrings: [
        '# renovate: datasource=(?<datasource>[a-z-]+?) depName=(?<depName>.+?)(?: packageName=(?<packageName>.+?))?(?: registryUrl=(?<registryUrl>.+?))?(?: versioning=(?<versioning>[a-z-]+?))?\\n\\s*.+\\s*=\\s*(?<currentValue>.+$)?',
      ],
    },
  ],
}
