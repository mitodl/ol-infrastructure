resources:
  io_manager:
    config:
      s3_bucket: {{ keyOrDefault "edx-pipeline/mitx-enrollments/bucket-name" "missing-value-bucket-name" }}
      s3_prefix: pipeline-storage/mitx-bigquery
  bigquery_db:
    config: {{ with secret "secret-operations/data/institutional-research-bigquery-service-account" }}{{ .Data.value | toYAML | indent 6 }}{{ end }}
ops:
  export_person_course:
    config:
      outputs_dir: /tmp/bigquery_person_course
      table_name: person_course
  export_user_info_combo:
    config:
      outputs_dir:  /tmp/bigquery_user_info_combo
      table_name: user_info_combo
