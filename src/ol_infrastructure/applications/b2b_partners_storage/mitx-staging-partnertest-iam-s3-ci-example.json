{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "ListMITBucketWithPrefix",
        "Effect": "Allow",
        "Action": "s3:ListBucket",
        "Resource": "arn:aws:s3:::ol-b2b-partners-storage-ci",
        "Condition": {
          "StringLike": {
            "s3:prefix": [
              "b2btestpartner1/*",
              "b2btestpartner1"
            ]
          }
        }
      },
      {
        "Sid": "ReadMITBucketObjects",
        "Effect": "Allow",
        "Action": [
          "s3:GetObject",
          "s3:GetObjectVersion",
          "s3:GetObjectTagging",
          "s3:GetObjectVersionTagging"
        ],
        "Resource": "arn:aws:s3:::ol-b2b-partners-storage-ci/b2btestpartner1/*"
      }
    ]
}
